# MAC Assistant - Environment Variables
# Copy this file to .env and add your API keys

# OpenAI API Key for ChatGPT integration
# Get from: https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# SerpAPI Key for Google Search integration (optional)
# Get from: https://serpapi.com/
SERPAPI_KEY=your_serpapi_key_here

# YouTube API Key (optional - uses free search if not provided)
# Get from: https://console.developers.google.com/
YOUTUBE_API_KEY=your_youtube_api_key_here

# OpenWeatherMap API Key for weather information (optional)
# Get from: https://openweathermap.org/api
OPENWEATHER_API_KEY=your_openweather_api_key_here

# Additional settings
MAC_DEBUG=false
MAC_VOICE_RATE=200
MAC_VOICE_VOLUME=0.9
